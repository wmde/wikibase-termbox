{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "sourceMap": true,
    "lib": [
      "esnext",
	  "es5",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
	  "src/**/*.ts",
	  "src/**/*.tsx",
	  "src/**/*.vue",
	  "tests/**/*.ts",
	  "tests/**/*.ts",
	  "tests/**/*.tsx"
  ],
  "exclude": [
	  "node_modules"
  ]
}
