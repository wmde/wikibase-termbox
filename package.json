{
  "name": "wikibase-termbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "WEBPACK_TARGET=dev vue-cli-service serve",
    "dev-server": "concurrently \"tsc -w --project tsconfig.server.json\" \"nodemon serverBuild/server/server.js\"",
    "build": "rm -rf node_modules/.cache && NODE_ENV=production vue-cli-service build",
    "build-server": "rm -rf node_modules/.cache && NODE_ENV=production WEBPACK_TARGET=node vue-cli-service build && tsc -p tsconfig.server.json",
    "lint": "tslint --format codeFrame --project tsconfig.all.json -c tslint.json",
    "fix": "tslint --format codeFrame --fix --project tsconfig.all.json -c tslint.json",
    "test": "vue-cli-service test:unit"
  },
  "_moduleAliases": {
    "@": "serverBuild/"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "module-alias": "^2.1.0",
    "mwbot": "^1.0.10",
    "rtl-detect": "^1.0.2",
    "ts-deferred": "^1.0.4",
    "tslib": "^1.9.3",
    "validate.js": "^0.12.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.4",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.6",
    "@types/supertest": "^2.0.6",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "concurrently": "^4.0.1",
    "jest-dom": "^2.1.1",
    "nock": "^10.0.2",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.7",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.0.1",
    "supertest": "^3.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.1.2",
    "vue-template-compiler": "^2.5.17"
  }
}
