{
  "name": "wikibase-termbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "WEBPACK_TARGET=dev vue-cli-service serve",
    "dev-server": "concurrently \"tsc -w --project tsconfig.server.json\" \"nodemon serverBuild/server.js\"",
    "build": "NODE_ENV=production vue-cli-service build",
    "build-server": "WEBPACK_TARGET=node vue-cli-service build && tsc --project tsconfig.server.json",
    "lint-client": "tslint --project tsconfig.json -c tslint.json",
    "fix-client": "tslint --fix --project tsconfig.json -c tslint.json",
    "lint-server": "tslint --project tsconfig.server.json -c tslint.json",
    "fix-server": "tslint --fix --project tsconfig.server.json -c tslint.json",
    "lint": "npm run lint-client; npm run lint-server",
    "fix": "npm run fix-client; npm run fix-server",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "only": "0.0.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
	"@types/node": "^10.12.6",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.4",
    "@types/supertest": "^2.0.6",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "concurrently": "^4.0.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.4",
    "sass-loader": "^7.0.1",
    "supertest": "^3.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.1.2",
    "vue-template-compiler": "^2.5.17"
  }
}
